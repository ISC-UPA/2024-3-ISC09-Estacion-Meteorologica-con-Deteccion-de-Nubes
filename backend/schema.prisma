// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model Login {
  id        String @id @default(cuid())
  idUser    Int    @unique
  nombre    String @default("")
  username  String @default("")
  passsword String @default("")
}

model Ubicacione {
  id         String @id @default(cuid())
  idLocacion Int    @unique
  nombre     String @default("")
  latitud    Int
  longitud   Int
}

model Temperatura {
  id            String   @id @default(cuid())
  idTemperatura Int      @unique
  valor         Int
  timestamp     DateTime
  idLocacion    String   @default("")
}

model Humedade {
  id         String   @id @default(cuid())
  idHumedad  Int      @unique
  valor      Int
  timestamp  DateTime
  idLocacion String   @default("")
}
